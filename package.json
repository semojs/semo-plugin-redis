{
  "name": "semo-plugin-redis",
  "version": "1.0.3",
  "description": "A Semo plugin to provide integration with Redis.",
  "main": "lib/index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.17",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/eslint-plugin": "^1.1.44",
    "@vitest/ui": "^3.1.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3"
  },
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "clean": "rimraf lib",
    "test": "vitest run",
    "test:watch": "vitest ",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run clean && npm run build",
    "prepare": "husky",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js}\"",
    "format:check": "prettier --check \"**/*.{ts,js}\"",
    "deploy": "npm version patch",
    "deploy:minor": "npm version minor",
    "deploy:major": "npm version major"
  },
  "dependencies": {
    "@semo/core": "^2.0.18",
    "ioredis": "^5.6.1",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public"
  }
}
